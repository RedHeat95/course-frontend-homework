const WRONG_PASSWORD_LANGUAGE = "        ";
const WRONG_PASSWORD_FORMAT = "     1 , 1   1  ";
const WRONG_PASSWORD_LENGTH = "     8     30 ";
const WRONG_REPEATED_PASSWORD = "  ";
const NOT_ERROR = "";

class ValidationService {
  validateRequired(value) {
    if (!value) {
      return REQUIRED_FIELD;
    }

    return NOT_ERROR;
  }

  validateLength(value, max, min) {
    if (!value) {
      return REQUIRED_FIELD;
    }

    if (max !== undefined) {
      return value.length > max ? WRONG_MAX_LENGTH(max) : NOT_ERROR;
    }

    if (min !== undefined) {
      return value.length < min ? WRONG_MIN_LENGTH(min) : NOT_ERROR;
    }

    return NOT_ERROR;
  }

  validateName(name) {
    if (!name) {
      return REQUIRED_FIELD;
    }

    if (name.length > 60) {
      return WRONG_NAME;
    }

    return NOT_ERROR;
  }

  validateEmail(email) {
    if (!email) {
      return ENTER_EMAIL;
    }

    if (email.length > 40) {
      return WRONG_EMAIL_LENGTH;
    }

    if (!_emailLanguageRegExp.test(email)) {
      return WRONG_EMAIL_LANGUAGE;
    }

    if (!_emailRegExp.test(email)) {
      return WRONG_EMAIL;
    }

    return NOT_ERROR;
  }

  validatePassword(password) {
    if (!password) {
      return ENTER_PASSWORD;
    }

    if (!_passwordLanguageRegExp.test(password)) {
      return WRONG_PASSWORD_LANGUAGE;
    }

    if (!_passwordRegExp.test(password)) {
      return WRONG_PASSWORD_FORMAT;
    }

    if (password.length < 8) {
      return WRONG_PASSWORD_LENGTH;
    }

    if (password.length > 30) {
      return WRONG_PASSWORD_LENGTH;
    }

    return NOT_ERROR;
  }

  validateRepeatedPassword(password, repeatedPassword) {
    if (!password) {
      return NOT_ERROR;
    }

    if (!repeatedPassword) {
      return ENTER_REPEATED_PASSWORD;
    }

    if (password !== repeatedPassword) {
      return WRONG_REPEATED_PASSWORD;
    }

    return NOT_ERROR;
  }

}

