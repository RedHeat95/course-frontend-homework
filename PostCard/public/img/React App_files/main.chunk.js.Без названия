function debounce(fun, ms) {
  let isCooldown = false;
  let prevSearchText = "";
  return function (searchText) {
    prevSearchText = searchText;

    if (isCooldown) {
      return;
    }

    fun(searchText);
    isCooldown = true;
    setTimeout(() => {
      isCooldown = false;

      if (prevSearchText !== searchText) {
        fun(prevSearchText);
      }
    }, ms);
  };
}

